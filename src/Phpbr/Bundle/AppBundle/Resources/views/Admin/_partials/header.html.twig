<header>
    <div class="contain-to-grid">
        <nav class="top-bar" data-topbar role="navigation">
            <ul class="inline-list left fonte-header" style="padding-top:10px;">
                <li><a href="{{ url('phpbr_app_inicial') }}">{{ 'phpbr.header.home'|trans({}, 'header') }}</a></li>
                <li><a href="{{ url('home_admin') }}">{{ 'phpbr.header.admin'|trans({}, 'header') }}</a></li>
            </ul>

            <section class="top-bar-section">
                <ul class="right">
                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                        <li>
                            <a href="{{ url('fos_user_security_logout') }}">
                                {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                            </a>
                        </li>
                    {% endif %}
                </ul>
            </section>
        </nav>
    </div>
</header>

<div id="content-wrapper">
    <div class="panel">
        <div class="row">
            <div class="large-12 columns">
                <h2>{{ 'phpbr.header.panel'|trans({}, 'header') }}</h2>
            </div>
        </div>
    </div>
</div>
