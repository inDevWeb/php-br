{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Cole</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ entity.titulo }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ entity.codigo }}</td>
            </tr>
            <tr>
                <th>Datacriacao</th>
                <td>{{ entity.dataCriacao|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Chavedeletar</th>
                <td>{{ entity.chaveDeletar }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cole') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cole_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
